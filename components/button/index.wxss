.comm-button{
	width: 100%;
	font-size: 36rpx;
	color: #fff;
	height: 80rpx;
	line-height: 80rpx;
	background: #ff5400;
	border: solid #ff5400 1rpx;
	border-radius: 12rpx;
	margin-bottom: 30rpx;
	text-align: center;
}
.comm-button.border{
	color: #ff5400;
	background: #fff;
}
.comm-button.disabled{
	background: #ccc;
	border: solid #ccc 1rpx;
}