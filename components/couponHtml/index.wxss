.comm-coupon-html{
	position: relative;
}
.comm-coupon-html .textarea{
	font-size: 36rpx;
	line-height: 70rpx;
	min-height: 88rpx;
	width: 100%;
	border-bottom: #ccc solid 1rpx;
	padding-bottom: 10rpx;
	padding-top: 60rpx;
	word-wrap: break-word;
	word-break: break-all;
	display: inline-block;
}
.comm-coupon-html .textarea.placeholder{
	color: #ccc;
}
.comm-coupon-html .textareaHover{
	background: #ccc;
}
.comm-coupon-html .couter{
	font-size: 26rpx;
	width: 100%;
	padding-bottom: 20rpx;
	text-align: right;
	display: block;
	pointer-events: none;
}
.comm-coupon-html .delete{
	width: 60rpx;
	height: 60rpx;
	background-color: rgba(0, 0, 0, .5);
	background-image: url(data:image/png;base64,);
	background-position: center;
	background-repeat: no-repeat;
	-webkit-background-size: 40rpx;
	background-size: 40rpx;
	position: absolute;
	right: 0;
	top: 0;
}
.comm-coupon-html .delete.transparent{
	background-color: transparent;
	background-image: url(data:image/jpeg;base64,);
}
.comm-coupon-html .upload{
	color: #ccc;
	background: #f5f5f7;
	width: 100%;
	height: 235rpx;
	line-height: 40rpx;
	text-align: center;
	overflow: hidden;
}
.comm-coupon-html .upload image{
	width: 84rpx;
	height: 84rpx;
	margin: 45rpx auto 30rpx;
	display: block;
}
.comm-coupon-html .delete{
	top: 0;
	margin-top: 0;
	z-index: 9;
}