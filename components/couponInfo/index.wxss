.comm-coupon-info{}
.comm-coupon-info .title{
	color: #999;
	font-size: 36rpx;
	line-height: 80rpx;
	text-indent: 20rpx;
}
.comm-coupon-info .banner{
	background: #fff;
}
.comm-coupon-info .banner image{
	width: 750rpx;
	height: 750rpx;
}
.comm-coupon-info .banner image.expand{
	width: 700rpx;
	height: 700rpx;
	margin: 25rpx;
}
.comm-coupon-info .banner image.expand:nth-of-type(n+2){
	margin-top: 0;
}
.comm-coupon-info .form{
	font-size: 32rpx;
	line-height: 100rpx;
	border-top: #f5f5f5 solid 10rpx;
	padding: 0 40rpx;
	-webkit-display: flex;
	display: flex;
	overflow: hidden;
}
.comm-coupon-info .form .label{
	color: #999;
	min-width: 140rpx;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
.comm-coupon-info .form .label .text{
	border-bottom: solid transparent 1rpx;
}
.comm-coupon-info .form .value{
	-webkit-flex: 1;
	flex: 1;
}
.comm-coupon-info .form .value .text{
	max-width: 500rpx;
	min-height: 100rpx;
	text-indent: 30rpx;
	border-bottom: solid #eee 1rpx;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
.comm-coupon-info .form .value .text.extend{
	line-height: 60rpx;
	padding: 20rpx 0;
	padding-left: 30rpx;
	text-indent: 0;
	white-space: initial;
	word-wrap: break-word;
	word-break: break-all;
}
.comm-coupon-info .html{
	padding: 30rpx 30rpx 200rpx;
}
.comm-coupon-info .html .node{
	font-size: 36rpx;
	line-height: 80rpx;
	word-wrap: break-word;
	word-break: break-all;
}
.comm-coupon-info .html .node image{
	width: 100%;
	display: block;
}
.comm-coupon-info .html .node .content{
	padding: 30rpx 0;
}

