.comm-launch-append{
	width: 100%;
	-webkit-display: flex;
	display: flex;
}
.comm-launch-append .prepend{
	width: 75rpx;
}
.comm-launch-append .prepend:before{
	content: '';
	width: 40rpx;
	height: 40rpx;
	border: solid #ccc 1rpx;
	border-radius: 50%;
	margin-top: 40rpx;
	margin-left: 2rpx;
	display: block;
}
.comm-launch-append.selected .prepend:before,
.comm-launch-append .prepend.selected:before{
	color: #fff;
	background: #ff5400;
	background-image: url('data:image/png;base64,');
	background-position: center;
	background-repeat: no-repeat;
	-webkit-background-size: 27rpx 19rpx;
	background-size: 27rpx 19rpx;
	border: solid #ff5400 1rpx;
}
.comm-launch-append .body{
	width: 440rpx;
	border-right: solid #f7f7f7 1rpx;
}
.comm-launch-append .body .left{
	color: #999;
	display: inline-block;
}
.comm-launch-append .body .leftNum{
	color: #ff5400;
	font-weight: bold;
	margin-left: 10rpx;
	display: inline-block;
}
.comm-launch-append .append{
	line-height: 70rpx;
	padding-left: 35rpx;
	text-align: center;
	-webkit-flex: 1;
	flex: 1;
}
.comm-launch-append .append .couponNum{
	color: #ccc;
	height: 100rpx;
	font-size: 36rpx;
	line-height: 100rpx;
	background: #f5f5f5;
	border-radius: 10rpx;
}
.comm-launch-append .append .couponNum.active{
	color: #000;
}