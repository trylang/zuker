.comm-nav{
	width: 100%;
	height: 95rpx;
	background: #fff;
	-webkit-display: flex;
	display: flex;
	position: fixed;
	left: 0;
	bottom: 0;
	-webkit-box-shadow: 0 0 10rpx #ccc;
	box-shadow: 0 0 10rpx #ccc;
}
.comm-nav.ipx{
	height: 155rpx;
}
.comm-nav .item{
	color: #999;
	font-size: 24rpx;
	padding-top: 10rpx;
	text-align: center;
	-webkit-flex: 1;
	flex: 1;
}
.comm-nav .item.active{
	color: #19325e;
}
.comm-nav .item image{
	width: 45rpx;
	height: 45rpx;
	margin: 0 auto;
	display: block;
}
.comm-nav .item image.icon{
	width: 104rpx;
	height: 104rpx;
	margin-top: -30rpx;
}

.item-noimg-label {
	line-height: 80rpx;
}
