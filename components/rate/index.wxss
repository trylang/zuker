.comm-rate{
	width: 100%;
	border: none;
	-webkit-display: flex;
	display: flex;
}
.comm-rate view{
	height: 37rpx;
	border: none;
	background: url(data:image/jpeg;base64,) center no-repeat;
	-webkit-background-size: 37rpx;
	background-size: 37rpx;
	-webkit-flex: 1;
	flex: 1;
}
.comm-rate view.active:before{
	content: '';
	width: 100%;
	height: 37rpx;
	background: url(data:image/jpeg;base64,) center no-repeat;
	-webkit-background-size: 37rpx;
	background-size: 37rpx;
	display: block;
}