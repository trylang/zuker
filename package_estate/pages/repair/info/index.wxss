.container{
	padding: 30rpx;
}
.container .title{
	font-size: 40rpx;
	font-weight: bold;
	line-height: 50rpx;
	white-space: nowrap;
	-ms-text-overflow: ellipsis;
	text-overflow: ellipsis;
	overflow: hidden;
}
.container .tag{
	font-size: 26rpx;
	line-height: 50rpx;
	display: inline-block;
}
.container .tag view.blue,
.container .tag view.red{
	line-height: 30rpx;
	border-style: solid;
	border-width: 1rpx;
	border-radius: 8rpx;
	padding: 2rpx 5rpx;
	margin-left: 10rpx;
	vertical-align: middle;
	display: inline-block;
}
.container .tag view:first-of-type{
	margin-left: 0;
}
.container .tag view.blue{
	color: #3a6dc4;
	border-color: #3a6dc4;
}
.container .tag view.red{
	color: #ff5400;
	border-color: #ff5400;
}
.container .label{
	color: #a0a0a0;
	margin-left: 20rpx;
	padding-right: 15rpx;
	display: inline-block;
}
.container .label:first-of-type{
	margin: 0;
}
.container .person{
	line-height: 70rpx;
}
.container .timeline{
	width: 100%;
	padding-top: 30rpx;
	text-align: center;
	-webkit-display: flex;
	display: flex;
}
.container .timeline .item{
	color: #999;
	margin-left: 70rpx;
	-webkit-flex: 1;
	flex: 1;
	position: relative;
}
.container .timeline .item:first-of-type{
	margin: 0;
}
.container .timeline .item:before{
	content: '';
	width: 17rpx;
	height: 17rpx;
	border: solid #ff5400 1rpx;
	background: #fff;
	border-radius: 50%;
	margin: 20rpx auto;
	display: block;
	position: relative;
	z-index: 9;
}
.container .timeline .item.active:before{
	background: #ff5400;
}
.container .timeline .item:after{
	content: '';
	width: 150%;
	height: 1rpx;
	background: #ff5400;
	overflow: hidden;
	display: block;
	position: absolute;
	left: -25%;
	top: 29rpx;
}
.container .timeline .item:first-of-type:after{
	width: 100%;
	left: 50%;
}
.container .timeline .item:last-of-type:after{
	width: 100%;
	left: -50%;
}
.container .timeline .item .time{
	color: #000;
}
.container .worker{
	line-height: 80rpx;
	margin: 30rpx -30rpx 0;
	padding: 0 30rpx;
	background: #efefef;
}
.container .workInfo{
	line-height: 80rpx;
	margin: 30rpx -30rpx 0;
	padding: 20rpx 30rpx;
	background: #efefef;
}
.container .workInfo .content{
	font-size: 40rpx;
	color: #000;
	word-wrap: break-word;
	word-break: break-all;
}
.container .workInfo .content image{
	width: 100%;
	margin-top: 15rpx;
}
.container .workInfo .content image:first-of-type{
	margin: 0;
}

.container .workInfo .content .orderStep{
	font-size: 0;
	line-height: 40rpx;
	margin-top: 40rpx;
}
.container .workInfo .content .orderStep:first-of-type{
	margin: 0;
}
.container .workInfo .content .orderStep .time,
.container .workInfo .content .orderStep .spacer,
.container .workInfo .content .orderStep .info{
	font-size: 30rpx;
	display: inline-block;
	vertical-align: top;
}
.container .workInfo .content .orderStep .time{
	font-size: 28rpx;
	color: #999;
}
.container .workInfo .content .orderStep .spacer .icon{
	width: 58rpx;
	height: 58rpx;
	background: #fff;
	border: solid #ccc 2rpx;
	border-radius:  50%;
	margin: 0 20rpx;
	position: relative;
}
.container .workInfo .content .orderStep.active .spacer .icon{
	background: #ff5400;
	border: none;
}
.container .workInfo .content .orderStep .spacer .icon:after{
	content: '';
	width: 2rpx;
	height: 100rpx;
	background: #ccc;
	position: absolute;
	left: 50%;
	margin-left: -1rpx;
	top: 100%;
	display: block;
}
.container .workInfo .content .orderStep:last-of-type .spacer .icon:after{
	display: none;
}
.container .workInfo .content .orderStep .spacer .icon image{
	width: 36rpx;
	height: 36rpx;
	position: absolute;
	left: 50%;
	margin-left: -18rpx;
	top: 50%;
	margin-top: -18rpx;
}
.container .workInfo .content .orderStep .info{
	color: #666;
	line-height: 50rpx;
}


.rateContent{
	font-size: 0;
	padding: 10rpx 0;
}
.rateContent .title,
.rateContent .comp,
.rateContent .desc{
	font-size: 30rpx;
	font-weight: normal;
	vertical-align: middle;
	display: inline-block;
}
.rateContent .title{
	width: 150rpx;
}
.rateContent .comp{
	width: 340rpx;
}
.rateContent .desc{
	color: #999;
	width: 130rpx;
}
.urge.icon{
	width: 100rpx;
	height: 100rpx;
	border: solid #ff874c 2rpx;
	border-radius: 50%;
	background: #fff3ec;
	margin: 0 auto 30rpx;
	-webkit-transform: rotate(45deg);
	transform: rotate(45deg);
	position: relative;
}
.urge.icon.return{
	background: #fff3ec url(data:image/jpeg;base64,) center no-repeat;
	-webkit-background-size: 50rpx;
	background-size: 50rpx;
	-webkit-transform: rotate(0deg);
	transform: rotate(0deg);
}
.urge.icon.success:before{
	content: '';
	width: 20rpx;
	height: 50rpx;
	border-right: solid #ff874c 5rpx;
	border-bottom: solid #ff874c 5rpx;
	display: block;
	position: absolute;
	left: 50%;
	margin-left: -10rpx;
	top: 50%;
	margin-top: -30rpx;
}
.urge.icon.fail:before,
.urge.icon.fail:after{
	content: '';
	border-radius: 5rpx;
	background: #ff874c;
	overflow: hidden;
	display: block;
	position: absolute;
	left: 50%;
	top: 50%;
}
.urge.icon.fail:before{
	width: 40rpx;
	height: 5rpx;
	margin-left: -20rpx;
	margin-top: -2.5rpx;
}
.urge.icon.fail:after{
	width: 5rpx;
	height: 40rpx;
	margin-left: -2.5rpx;
	margin-top: -20rpx;
}
.bottomer{
	width: 100%;
	height: 150rpx;
}





