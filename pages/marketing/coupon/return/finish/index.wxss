.header{
	margin: 55rpx 30rpx;
}
.body{
	border-bottom: solid #efefef 1rpx;
	padding-bottom: 50rpx;
	margin: 50rpx;
	position: relative;
}

.body .success{
	width: 100rpx;
	height: 100rpx;
	background: #f7fbf3;
	border: solid #b9dc8a 2rpx;
	border-radius: 50%;
	margin: 0 auto;
	position: relative;
}
.body .success:before{
	content: '';
	width: 20rpx;
	height: 50rpx;
	border-right: solid #b9dc8a 5rpx;
	border-bottom: solid #b9dc8a 5rpx;
	-webkit-transform: rotate(45deg) translate(-130%, -30%);
	transform: rotate(45deg) translate(-130%, -30%);
	display: block;
	position: absolute;
	left: 50%;
	top: 50%;
}

.body .title{
	font-size: 40rpx;
	line-height: 80rpx;
	font-weight: bold;
	text-align: center;
}
.body .coupon{
	min-height: 100rpx;
	border: solid #e2e2e2 1rpx;
	border-radius: 10rpx;
	padding: 50rpx 30rpx;
	margin-top: 30rpx;
	position: relative;
}
.body .coupon:before,
.body .coupon:after{
	content: '';
	width: 95%;
	height: 22rpx;
	background: url(data:image/jpeg;base64,) repeat-x;
	-webkit-background-size: 22rpx 11rpx;
	background-size: 22rpx 11rpx;
	position: absolute;
	left: 2.5%;
	display: block;
}
.body .coupon:before{
	top: -1rpx;
}
.body .coupon:after{
	-webkit-transform: scaleY(-1);
	transform: scaleY(-1);
	bottom: -1rpx;
}

.body .coupon .title{
	font-size: 36rpx;
	line-height: 60rpx;
}
.body .coupon .extend{
	color: #aaa;
  text-align: center;
  margin-bottom: 40rpx;
}

.body .coupon .id{
	font-size: 34rpx;
	font-weight: bold;
	color: #17305c;
	line-height: 60rpx;
	text-align: center;
}
.body .coupon .id .label{
	font-size: 30rpx;
	font-weight: normal;
	color: #aaa;
	margin-right: 10rpx;
	display: inline-block;
}
.body .coupon .id.normal{
	color: #000;
	font-size: 30rpx;
	font-weight: normal;
	text-align: center;
}

.footer{
	padding: 10rpx 30rpx;
}