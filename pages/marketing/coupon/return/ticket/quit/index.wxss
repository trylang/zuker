.header{
	margin: 55rpx 30rpx;
}
.body{
	border-bottom: solid #efefef 1rpx;
	padding-bottom: 50rpx;
	margin: 50rpx 50rpx;
	position: relative;
}
.body image{
	width: 140rpx;
	height: 140rpx;
	border-radius: 50%;
	margin: 0 auto;
	display: block;
}
.body .title{
	font-size: 34rpx;
	line-height: 60rpx;
	margin-top: 15rpx;
	text-align: center;
}
.body .id{
	font-weight: bold;
	font-size: 34rpx;
	color: #17305c;
	line-height: 60rpx;
	text-align: center;
}
.body .id .label{
	font-size: 24rpx;
	font-weight: normal;
	color: #999;
	margin-right: 10rpx;
	display: inline-block;
}
.footer{
	padding: 10rpx 30rpx;
}
.body .coupon {
  margin-top: 60rpx;
}
.body .coupon .id{
	font-size: 34rpx;
	font-weight: bold;
	color: #17305c;
	line-height: 60rpx;
	text-align: left;
}
.body .coupon .id .label{
	font-size: 30rpx;
	font-weight: normal;
	color: #999;
	margin-right: 10rpx;
	display: inline-block;
}

.body .coupon .id.normal{
	color: #282828;
	font-size: 30rpx;
	text-align: left;
}

.body .coupon .id.explain {
	width: 100%;
}

.body .coupon .id.explain .explain-item{
	display: block;
}

.footer .couponList .radio .detail .info .val {
  display: inline;
  font-size: 30rpx;
  font-weight:bold;
}
